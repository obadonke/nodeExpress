{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": 2,
        "no-console": "off",
        "brace-style": "error",
        "space-before-blocks": "warn"
    },
    "env": {
        "browser": true,
        "node": true
    }
}
